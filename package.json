{
  "name": "github-dashboard",
  "version": "1.2.0",
  "description": "A GitHub Dashboard web app running on-top of Node",
  "main": "src/index.tsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thescientist13/github-dashboard.git"
  },
  "engines": {
    "node": ">=4.4.1"
  },
  "keywords": [
    "Node",
    "Github",
    "The Greenhouse"
  ],
  "author": "Owen Buckley <owen.buckley@analogstudios.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/thescientist13/github-dashboard/issues"
  },
  "homepage": "https://github.com/thescientist13/github-dashboard#readme",
  "scripts": {
    "clean": "rimraf ./build && rimraf ./reports && mkdir ./reports && mkdir ./reports/test-results",
    "build": "webpack --config ./webpack.config.common.js --progress --profile --bail",
    "develop": "webpack-dev-server --open --config ./webpack.config.develop.js --inline --progress",
    "serve": "webpack --config ./webpack.config.prod.js -p --progress --profile --bail && http-server ./build -o",
    "setup": "rimraf ./src/services/credentials.ts && cp ./src/services/credentials.ts.tmpl ./src/services/credentials.ts",
    "test": "yarn clean && export TEST_REPORT_PATH=./reports/test-results && eslint ./src/**/**/*.spec.jsx && jest --coverage"
  },
  "dependencies": {
    "axios": "0.15.2",
    "bootstrap": "3.3.7",
    "react": "15.4.0",
    "react-dom": "15.4.0",
    "react-redux": "4.4.6",
    "react-router": "3.0.2",
    "redux": "3.6.0"
  },
  "devDependencies": {
    "@types/axios": "0.9.33",
    "@types/react": "0.14.49",
    "@types/react-addons-test-utils": "^0.14.21",
    "@types/react-dom": "0.14.18",
    "@types/react-redux": "4.4.32",
    "@types/react-router": "3.0.5",
    "@types/redux": "3.6.31",
    "awesome-typescript-loader": "^5.0.0",
    "axios-mock-adapter": "1.7.1",
    "babel-jest": "19.0.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "css-loader": "^0.28.11",
    "enzyme": "2.7.1",
    "eslint": "3.18.0",
    "eslint-plugin-react": "6.10.3",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "0.9.0",
    "jest": "19.0.2",
    "jest-junit-reporter": "1.0.1",
    "pipeline-validate-js": "1.0.4",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^15.4.2",
    "redux-mock-store": "^1.2.2",
    "rimraf": "2.5.4",
    "source-map-loader": "0.1.5",
    "style-loader": "^0.21.0",
    "ts-jest": "19.0.0",
    "typescript": "^2.9.2",
    "typings": "1.4.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.6",
    "webpack-dev-server": "^3.1.4",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "0.14.1"
  }
}
