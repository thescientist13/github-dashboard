{
  "name": "github-dashboard",
  "version": "0.4.0",
  "description": "A Github Dashboard app running on-top of Node",
  "main": "src/bootstrap.tsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thescientist13/github-dashboard.git"
  },
  "engines": {
    "node": ">=4.4.1"
  },
  "keywords": [
    "Node",
    "Github",
    "The Greenhouse"
  ],
  "author": "Owen Buckley <owen@analogstudios.net>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thescientist13/github-dashboard/issues"
  },
  "homepage": "https://github.com/thescientist13/github-dashboard#readme",
  "scripts": {
    "build": "rimraf ./build && webpack --config ./webpack.config.prod.js -p --progress --profile --bail",
    "ci": "export NODE_ENV=production && npm run build",
    "develop": "export NODE_ENV=development && rimraf ./build && webpack-dev-server --config ./webpack.config.dev.js --inline --progress --port 4567",
    "demo": "npm run build && ./node_modules/.bin/gulp serve"
  },
  "dependencies": {
    "axios": "0.15.2",
    "bootstrap": "3.3.7",
    "react": "15.4.0",
    "react-dom": "15.4.0",
    "react-router": "3.0.0"
  },
  "devDependencies": {
    "@types/react": "0.14.49",
    "@types/react-dom": "0.14.18",
    "@types/react-router": "2.0.39",
    "awesome-typescript-loader": "2.2.4",
    "babel-core": "6.7.7",
    "babel-preset-es2015": "6.6.0",
    "css-loader": "0.26.0",
    "file-loader": "0.9.0",
    "gulp": "3.9.1",
    "gulp-webserver": "0.9.1",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.22.0",
    "rimraf": "2.5.4",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "ts-loader": "0.8.2",
    "typescript": "2.0.10",
    "typings": "1.4.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "0.14.1"
  }
}