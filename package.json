{
  "name": "github-dashboard",
  "version": "1.0.0-rc2",
  "description": "A GitHub Dashboard web app running on-top of Node",
  "main": "src/index.tsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thescientist13/github-dashboard.git"
  },
  "engines": {
    "node": ">=4.4.1"
  },
  "keywords": [
    "Node",
    "Github",
    "The Greenhouse"
  ],
  "author": "Owen Buckley <owen.buckley@analogstudios.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/thescientist13/github-dashboard/issues"
  },
  "homepage": "https://github.com/thescientist13/github-dashboard#readme",
  "scripts": {
    "clean": "rimraf ./build && rimraf ./reports && mkdir ./reports && mkdir ./reports/test-results",
    "build": "yarn run clean && yarn run test -- --coverage && webpack --config ./webpack.config.prod.js -p --progress --profile --bail",
    "lint:jsx": "eslint ./src/**/**/*.spec.jsx",
    "develop": "webpack-dev-server --config ./webpack.config.dev.js --inline --progress",
    "serve": "webpack --config ./webpack.config.prod.js -p --progress --profile --bail && http-server ./build -o",
    "setup": "rimraf ./src/services/credentials.ts && cp ./src/services/credentials.ts.tmpl ./src/services/credentials.ts",
    "test": "yarn run lint:jsx && export TEST_REPORT_PATH=./reports/test-results && jest --config ./jest.config.json"
  },
  "dependencies": {
    "axios": "0.15.2",
    "bootstrap": "3.3.7",
    "react": "15.4.0",
    "react-dom": "15.4.0",
    "react-redux": "4.4.6",
    "react-router": "3.0.2",
    "redux": "3.6.0"
  },
  "devDependencies": {
    "@types/axios": "0.9.33",
    "@types/react": "0.14.49",
    "@types/react-dom": "0.14.18",
    "@types/react-redux": "4.4.32",
    "@types/react-router": "3.0.5",
    "@types/redux": "3.6.31",
    "awesome-typescript-loader": "2.2.4",
    "axios-mock-adapter": "1.7.1",
    "babel-jest": "19.0.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "css-loader": "0.26.0",
    "enzyme": "2.7.1",
    "eslint": "3.18.0",
    "eslint-plugin-react": "6.10.3",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.22.0",
    "http-server": "0.9.0",
    "jest": "19.0.2",
    "jest-junit-reporter": "1.0.1",
    "pipeline-validate-js": "1.0.4",
    "react-addons-test-utils": "15.4.2",
    "rimraf": "2.5.4",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "ts-jest": "19.0.0",
    "ts-loader": "0.8.2",
    "typescript": "2.0.10",
    "typings": "1.4.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "0.14.1"
  }
}